# -- page movement
move_down k
move_up j
move_right h
move_left l
# pages
next_page <C-d>
previous_page <C-u>
goto_toc to
prev_state H # prev in scroll history
next_state L # next in scroll history
# chapter
next_chapter gc
prev_chapter gC
# window
goto_window tw

# -- zoom
zoom_in +
zoom_out -
fit_to_page_width =
fit_to_page_width_smart <A-0>
toggle_fullscreen tf

# -- execute command
# put index char(like 'x') after 'r' to execute command predefined in prefs.config
execute_predefined_command r


# -- rotate
rotate_clockwise <C-r>
rotate_counterclockwise <C-S-r>

# -- mark
set_mark m
goto_mark '
# bookmark
add_bookmark b
delete_bookmark db
goto_bookmark gb
goto_bookmark_g gB
# highlight
add_highlight e
delete_highlight dh
goto_highlight gh
goto_highlight_g gH
toggle_highlight th

# -- file
open_prev_doc O  # show list of opened documents
open_document_embedded_from_current_path o # open file using embedded file browser
toggle_dark_mode td
toggle_presentation_mode tp
open_selected_url Q
close_window q
quit <C-S-w>

# -- editing
copy y
keyboard_select v # visual selection of selected word
keyboard_smart_jump F
enter_visual_mark_mode V
move_visual_mark_up j
move_visual_mark_down k
smart_jump_under_cursor f
# overview_under_cursor fo


# -- command
# :keys_user - open keymapping config file
# :prefs_user - open preference config file
