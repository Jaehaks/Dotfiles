	"environment": {
		"extension": "2.12.3",
		"platform": "win",
		"firefox": 140
	},
	"autoDarkMode": 1,
	"keyLayout": 2,
	"keyMappings": [
		"#!no-check",
		"########### scroll / navigate ##############",
		"# ]], [[ for next/previous page in lists doesn't work",
		"unmap <c-y>",
		"unmap <c-e>",
		"map k scrollDown",
		"map j scrollUp",
		"map <c-d> scrollPageDown",
		"map <c-u> scrollPageUp",
		"# m : mark current scroll location",
		"map ' Marks.activate",
		"map = visitPreviousTab",
		"# open link in new tab and switch it",
		"unmap F",
		"map F LinkHints.activateOpenInNewForegroundTab",
		"# open multiple link in new tab",
		"map <a-f> LinkHints.activateWithQueue",
		"",
		"# p : open clipboards url in current tab",
		"# P : open clipboard url in new tab",
		"# r : reload",
		"# t : create new tab",
		"# T : select tab (include other web instance)",
		"# / : search (n, N)",
		"# H : previous page",
		"# L : forward page",
		"",
		"############ managing #########",
		"unmap x",
		"unmap X",
		"map q removeTab",
		"map Q restoreTab",
		"map ww closeOtherTabs",
		"map wq closeTabsOnLeft",
		"map we closeTabsOnRight",
		"map t duplicateTab",
		"",
		"# W : move tab to new window",
		"# K : nextTab",
		"# J : previousTab",
		"# x : remove tab",
		"# X : restoreTab",
		"# ^ : previousvisitedTab",
		"",
		"",
		"############# zoom ############",
		"map zi zoomIn",
		"map zo zoomOut",
		"map z0 zoomReset",
		"",
		"############ focus ##########",
		"# gi : focus visible text box with colorful",
		"# F2 : focus previous focused block",
		"",
		"########### yank #########",
		"# v : visual mode, start with / to set initial cursor",
		"# c (in visual) : carot mode, to move cursor, ",
		"#                 after then, retry 'v' to insert in visual mode",
		"# yy : copy current url",
		"# yf : copy selected url using f",
		"# yi : copy image only",
		"# yv : select text to visual mode using f",
		"# Downloadimage doesn't work",
		"# p : open url from clipboard in current tab",
		"# P : open url from clipbaord in new tab",
		"",
		"######### vomnibar #########",
		"# open history in new tab",
		"# o : insert mode in url. open it / it can search anything",
		"#     it contains history / bookmark / search any",
		"# O : o in new tab",
		"# b : search in bookmark in currenttab",
		"# B : open bookmark in new tab",
		"# ge : edit current url",
		"# gE : edit current url and open new tab",
		"",
		"######### unmap #########",
		"unmap gn",
		"unmap <a-n>",
		"unmap <a-s-c>",
		"unmap <a-c>",
		"unmap <a-v>",
		"unmap <a-p>",
		"unmap gf",
		""
	],
	"searchEngines": [
		"g|go|gg|google|Google: https://www.google.com/search?q=%s \\",
		"  www.google.com re=/^(?:\\.[a-z]{2,4})?\\/search\\b.*?[#&?]q=([^#&]*)/i \\",
		"  blank=https://www.google.com/ Google",
		"y|yt: https://www.youtube.com/results?search_query=%s \\",
		"  blank=https://www.youtube.com/ YouTube",
		"w|wiki: https://www.wikipedia.org/w/index.php?search=%s Wikipedia",
		"gs|gscholar: https://scholar.google.com/scholar?q=$s \\",
		"  scholar.google.com re=/^(?:\\.[a-z]{2,4})?\\/scholar\\b.*?[#&?]q=([^#&]*)/i \\",
		"  blank=https://scholar.google.com/ Google Scholar",
		"gh|github: https://github.com/search?q=$s \\",
		"  blank=https://github.com/ GitHub Repo",
		"n|naver: https://search.naver.com/search.naver?query=%s",
		"nd: https://dict.naver.com/dict.search?dicQuery=%s&query=%s",
		"nm: https://namu.wiki/Go?q=%s",
		""
	],
	"smoothScroll": false,
	"vimSync": true
}
